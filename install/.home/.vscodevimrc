let mapleader = " "

" Leader to <Space>
""noremap <Space> <Nop>
" map <Space> <Leader>


 " Change VScode Ctrl binding to Space 
" :echo mapleader or :let mapleader

" Editing
nnoremap <leader>r <C-r>

" Numbers 
nnoremap <leader>a <C-a>
nnoremap <leader>x <C-x>

" Visual
nnoremap <leader>v <C-v>

" Windows
nnoremap <leader>w <C-w>

