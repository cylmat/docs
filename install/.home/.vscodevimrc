" define Space as Leader
 " Change VScode Ctrl binding to Space 
" :echo mapleader or :let mapleader
let mapleader = " "

" Leader to <Space>
""noremap <Space> <Nop>
" map <Space> <Leader>

" Set configuration in VsCode to disable plugin keys
"vim.handleKeys": {
    "<C-a>": false,
    "<C-c>": false,
    "<C-f>": false,
    "<C-o>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-z>": false
"}



nnoremap <leader>f :call VSCodeNotify('workbench.action.quickOpen')<CR>
nnoremap <leader>e :call VSCodeNotify('workbench.action.toggleSidebarVisibility')<CR>

" Editing
nnoremap <leader>r <C-r>

" Numbers 
nnoremap <leader>a <C-a>
nnoremap <leader>x <C-x>

" Visual
nnoremap <leader>v <C-v>

" Windows
nnoremap <leader>w <C-w>

" Sneak plugin
" nmap s <Plug>Sneak_s
" nmap S <Plug>Sneak_S
" map f <Plug>Sneak_s
" map F <Plug>Sneak_S

