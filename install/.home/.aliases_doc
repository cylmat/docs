alias netcheat='echo "
curl -L ipconfig.me

# lsof -i:22 tcp:3306
lsof -nP -i

# (n)o domain resolv (l)isten
nc -v -n 8.8.8.8 1-1000 
nc -l localhost 80      

# (p)rog (l)isten (u)dp (n)um (t)cp 
netstat -plunt
ss -plunt

# (a)ll (n)um (o)timer
netstat -ano

# (s)can TCP, (s)can UDP, enable (O)S detect
nmap -sTU -O 127.0.0.1

(i)nterface (n)o host convertion
tcpdump -n -i eth0
"'

